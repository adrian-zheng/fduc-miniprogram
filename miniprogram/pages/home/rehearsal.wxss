.tabs {
  background-image: linear-gradient(#7D0A32, #940839);
}

.vanBlock, .blockContent {
  --cell-group-inset-padding: 10rpx 30rpx 10rpx;
  --cell-background-color: #F5F5F5;
  --cell-border-color: #F5F5F5;
  /* --cell-font-size: 32rpx */
}

.van-cell-group .createAtt {
  height: 150px !important;
}

.goingOn .van-cell__value{
  --cell-value-color: #b15373;
  font-weight: bold;
}

.yet2Start {
  --cell-value-color: #b15373;
}

.vis {
  visibility: hidden;
}

.secondRow {
  display: flex;
  justify-content: space-between;
}

.cellTitle {
  color: #646566;
}

/* .padding-box {
  padding: 10rpx 130rpx 10rpx;
  height: 20rpx;
