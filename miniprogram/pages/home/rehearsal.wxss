} */

.profile-btn button{
  /* background: #ff495e; */
  background: #9e063b;
  color: #ffffff;
  width: 50vw;
  margin-top: 20rpx;
  border-radius: 50rpx;
  font-size: 14px;
}

.iconBack {
  width: 36px;
  height: 36px;
  border-radius: 50%;
  background-color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0 0 3px 2px rgba(0, 0, 0, 20%);
}

.add-wrap {
  width: 66px;
  height: 66px;
  position: fixed;
  right: 2vw;
  bottom: 90px;
  z-index: 9;
  border-radius: 120rpx 0rpx 0 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: flex-end;
  padding: 15rpx;
  border: #ffffff;
}

.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  z-index: 99999;
  border-radius: 5%;
}

.block {
  width: 80vw;
  height: 30vh;
  background-color: #fff;
  border-radius: 10px;
  overflow: hidden;
}

.block .van-cell__title {
  width: 250px;
}

.value-class {
  flex: none !important;
}

